{
    "name": "jespitia/createcrud",
    "description": "create CRUD",
    "license": "MIT",
    "keywords": ["crud", "laravel 5"],
    "homepage": "https://github.com/chuchoarte/createcrud",
    "authors": [
        {
            "name": "Jesus espitia",
            "email": "surjesus.sistemas@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~6.0",
        "sven/artisan-view": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "jespitia\\createcrud\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "jespitia\\createcrud\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jespitia\\createcrud\\CreateCRUDServiceProvider"
            ]
        }
    },
}
